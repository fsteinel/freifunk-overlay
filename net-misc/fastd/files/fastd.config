#see /usr/share/doc/fast*/fastd.config for an example

config fastd 'mesh_vpn'
	option mtu '1426'
	option group 'gluon-fastd'
	option status_socket '/var/run/fastd.mesh_vpn.socket'
	option packet_mark '1'
	option syslog_level 'info'
	option mode 'tap'
	option secure_handshake '1'
	option enabled '1'
	include '/etc/fastd/fastd.config.key
	list method 'salsa2012+umac'

config peer_group 'mesh_vpn_backbone'
	option enabled '1'
	option peer_limit '2'
	option net 'mesh_vpn'

config peer 'mesh_vpn_backbone_peer_ffki_vpn4'
        option enabled '1'
        option key '647b2af8c795a30b9f55758b1e59d9740e65c06bde6baec2c88136b12e974cb7'
        option net 'mesh_vpn'
        list remote 'ipv4 "vpn4.freifunk.in-kiel.de" port 10000'
        option group 'mesh_vpn_backbone'

